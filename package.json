{
  "name": "bimmer-charge",
  "version": "0.0.0-dev",
  "private": true,
  "scripts": {
    "start": "npm run prod",
    "build": "npm-run-all clean transpile",
    "server": "node ./dist-server/bin/www",
    "dev": "NODE_ENV=development npm-run-all build server",
    "prod": "NODE_ENV=production npm-run-all build server",
    "transpile": "babel ./server --out-dir dist-server",
    "clean": "rimraf dist-server",
    "watch:dev": "nodemon",
    "test": "jest"
  },
  "dependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "axios": "^0.26.0",
    "bmw-connected-drive": "^1.0.5",
    "cookie-parser": "~1.4.4",
    "core-js": "^3.21.1",
    "cors": "^2.8.5",
    "debug": "~4.3.3",
    "express": "~4.17.3",
    "morgan": "~1.10.0",
    "node-schedule": "^2.1.0",
    "npm-run-all": "^4.1.5",
    "prom-client": "^14.0.1",
    "prometheus-api-metrics": "^3.1.0",
    "regenerator-runtime": "^0.13.9",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.15"
  },
  "nodemonConfig": {
    "exec": "npm run dev",
    "watch": [
      "server/*",
      "public/*"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}
